{
  "workbench.colorTheme": "PyCharm Night Theme",
  "files.autoSave": "afterDelay",
  "editor.selectionClipboard": false,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDragAndDrop": false,
  "python.terminal.activateEnvInCurrentTerminal": false,
  "python.terminal.activateEnvironment": false,
  "python.createEnvironment.trigger": "off",
  "python.venvPath": "/dev/null",
  "python.defaultInterpreterPath": "/usr",
  "mypy.runUsingActiveInterpreter": true,
  "svelte.enable-ts-plugin": true,
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "explorer.confirmPasteNative": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "mypy.enabled": false,
  "ruff.lint.enable": false,
  "python.analysis.autoFormatStrings": true,
  "files.exclude": {
    "**/__pycache__/": true,
    "**/.git": false,
    "**/.git/": true,
    "**/.hg": false,
    "**/.hg/": true,
    "**/.svn": false,
    "**/.svn/": true,
    "**/.venv/": true,
    "**/.vscode/": true
  },
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "emmet.useInlineCompletions": true,
  "python.venvFolders": ["/dev/null"],
  "explorer.confirmDelete": false,
  "workbench.activityBar.location": "bottom",
  "workbench.secondarySideBar.showLabels": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "editor.codeActionsOnSave": {  
    "source.organizeImports.ruff": "explicit"
  },
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.allowChords": false,
  "python.interpreter.infoVisibility": "always",
  "gitlens.rebaseEditor.ordering": "asc",
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.startupEditor": "none",
  "window.titleBarStyle": "native",
  "chat.commandCenter.enabled": false,
  "C_Cpp.files.exclude": {
    "**/.vscode": true,
    "**/.vs": true,
    "iOS/Resources/": true
  },
  "dart.flutterSdkPath": "/home/bswck/.flutter-sdk/flutter",
  "python.analysis.typeEvaluation.strictListInference": true,
  "python.analysis.typeEvaluation.strictDictionaryInference": true,
  "python.analysis.typeEvaluation.strictSetInference": true,
  "python.analysis.regenerateStdLibIndices": true,
  "docker.extension.enableComposeLanguageServer": false,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "rust": false
  },
  "amp.url": "https://ampcode.com/",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.formatOnSaveMode": "modifications",
  "ruff.codeAction.fixViolation": {
    "enable": false
  },
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "errorLens.excludeBySource": [
    "[\"Pylance(reportShadowedImports)\"]"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true
}
